#include <iostream>

int main() {
    // Loop for numbers from 0 to 5
    for (int i = 0; i <= 5; i++) {
        std::cout << "Multiplication Table of " << i << ":" << std::endl;
        
        // Loop for multiplying from 1 to 10
        for (int j = 1; j <= 10; j++) {
            std::cout << i << " x " << j << " = " << i * j << std::endl;
        }
        
        std::cout << std::endl; // Print a blank line for better readability
    }
    
    return 0;
}
